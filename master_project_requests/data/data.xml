<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">

        <!-- Stages -->
        <record id="request_type_stage_new" model="getmo.request.type.stage">
            <field name="sequence">1</field>
            <field name="name">New</field>
            <field name="stage_type">draft</field>
        </record>

        <record id="request_type_stage_assigned" model="getmo.request.type.stage">
            <field name="sequence">2</field>
            <field name="name">Assigned</field>
            <field name="stage_type">assigned</field>
        </record>

        <record id="request_type_stage_in_progress" model="getmo.request.type.stage">
            <field name="sequence">3</field>
            <field name="name">In Progress</field>
            <field name="stage_type">in_progress</field>
        </record>

        <record id="request_type_stage_done" model="getmo.request.type.stage">
            <field name="sequence">4</field>
            <field name="name">Done</field>
            <field name="stage_type">done</field>
        </record>

        <record id="request_type_stage_refused" model="getmo.request.type.stage">
            <field name="sequence">5</field>
            <field name="name">Refused</field>
            <field name="stage_type">refused</field>
        </record>

    </data>
</odoo>